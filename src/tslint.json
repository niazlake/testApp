{
  "extends": "../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "app",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "app",
      "kebab-case"
    ],
    "no-any": [
      true,
      {
        "ignore-rest-args": true
      }
    ],
    "no-console": [
      true,
      "log",
      "error"
    ]
  }
}
